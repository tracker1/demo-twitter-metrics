{
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "deno.enable": true,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "files.associations": {
    ".env": "dotenv",
    ".env.*": "dotenv"
  }
}
